@using WebApp.ViewModels
@model WebApp.ViewModels.DashboardViewModel

@if (Model.SchedulingProcess == "SpecialRequest")
{
    @Html.Partial("_SpecialRequestForm")
}
else
{
    @Html.Partial("_ScheduleOnceForm")
}

@section scripts
{
    @if (Model.SchedulingProcess == "SpecialRequest")
    {
        @Html.Partial("_SpecialRequestForm_js")
    }
    else
    {
        @Html.Partial("_ScheduleOnceForm_js")
    }
}
