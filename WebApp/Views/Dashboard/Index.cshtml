                                                                                <strong class="text-warning">Waiting On: </strong>
                                                                                @foreach (var task in assessment.Tasks.Where(t => t.AssignedTo != Model.SelectedUserId && t.StatusId == TaskStatuses.ToDo).Select(t => new { t.AssignedTo, t.AssignedToDisplayName, t.AssignedToInitials, t.AssignedToColorCode }).Distinct())
                                                                                {
