@model CancellationPolicyViewModel

<dl>
    <dt>No Show Rate</dt>
    <dd>
        @Model.NoShowRate
    </dd>
    <dt>Late Cancellation Rate</dt>
    <dd>
        @Model.LateCancellationRate
    </dd>
    <dt>Late Cancellation Policy in Hours (eg. 72 hours = 3 days)</dt>
    <dd>
        @Model.LateCancellationPolicy hours
    </dd>
</dl>

