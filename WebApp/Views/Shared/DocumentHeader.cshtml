@model Orvosi.Data.Invoice
@{
    Layout = "~/Views/Shared/_LayoutPrint.cshtml";
    ViewBag.Title = "Header";
}

<style type="text/css">
    .logo-hsolo-md {
    display: block;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    background: url(data:image/jpeg;base64,) no-repeat;
    width: 600px; /* Width of new image */
    height: 100px; /* Height of new image */
    padding-left: 600px; /* Equal to width of new image */
}

.logo-zwaseem-md {
    display: block;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    background: url(data:image/jpeg;base64,) no-repeat;
    width: 600px; /* Width of new image */
    height: 100px; /* Height of new image */
    padding-left: 600px; /* Equal to width of new image */
}

.logo-sdessouki-md {
    display: block;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    background: url(data:image/jpeg;base64,) no-repeat;
    width: 600px; /* Width of new image */
    height: 100px; /* Height of new image */
    padding-left: 600px; /* Equal to width of new image */
}

.logo-rgandhi-md {
    display: block;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    background: url(data:image/jpeg;base64,) no-repeat;
    width: 600px; /* Width of new image */
    height: 100px; /* Height of new image */
    padding-left: 600px; /* Equal to width of new image */
}

    .right-header {
        float: right;
        font-family: Arial;
        text-align: right;
    }
</style>

<div style="display:block;">
    <div class="@Model.ServiceProviderLogoCssClass" style="float:left"></div>

    <div class="right-header">
        <strong>@Model.ServiceProviderName</strong>
        <p>
            <div class="small">@Model.ServiceProviderAddress1</div>
            <div class="small">@Model.ServiceProviderAddress2</div>
            <div class="small">@(string.Format("{0}, {1} {2}", Model.ServiceProviderCity, Model.ServiceProviderProvince, Model.ServiceProviderCountry))</div>
            <div class="small">@Model.ServiceProviderPostalCode</div>
        </p>
        <div><strong>E: </strong>@Model.ServiceProviderEmail</div>
    </div>
</div>