@using Box.V2.Models
@model BoxCollection<BoxUser>
@{
    ViewBag.Title = "Users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Users</h2>

<table class="table">
    @foreach (var item in Model.Entries)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Id</td>
            <td><a href="~/Box/LogInAs/@item.Id">Log In As</a></td>
        </tr>
    }
</table>
