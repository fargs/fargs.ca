@model WebApp.Areas.Admin.ViewModels.Role.AssignUsersViewModel

@foreach (var item in Model.Users)
{
    <tr>
        <td style="text-align: center; vertical-align: middle;">
            <input type="checkbox" name="@item.UserName" value="@item.UserId" checked="@item.IsAssigned" on />
        </td>
        <td style="vertical-align: middle">
            <span>
                @item.UserName - @item.Email
            </span>
        </td>
    </tr>
}
