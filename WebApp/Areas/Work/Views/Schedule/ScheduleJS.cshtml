<script>

        function getWeek(day, targetId, expand) {
            let url = '@Url.Content("~/Work/Schedule/Week")';

            var container = $(document.getElementById(targetId));
            if (container){
                if (!container.hasClass('in') || expand) {
                    $.post(url, {
                        firstDayOfWeek: day
                    })
                    .done(function (result) {
                        document.getElementById(targetId).innerHTML = result;
                        container.collapse('show');
                    });
                }
                else {
                    container.collapse('hide');
                }
            }
        }
    
        function showCaseDetails(serviceRequestId) {
            window.open('@Url.Content("~/ServiceRequest/Details/")' + serviceRequestId, '_Blank');
        }
    
</script>