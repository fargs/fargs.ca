@using WebApp.FormModels
@model CancellationForm

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <h4 class="modal-title">
        Delete Form
    </h4>
</div>
<div class="modal-body">
    <form id="delete-request-form">
        @Html.Hidden("ServiceRequestId", Model.ServiceRequestId)
    </form>
</div>
<div class="modal-footer">
    <button class="btn btn-success" onclick="deleteRequest($('#delete-request-form'))">Save</button>
</div>